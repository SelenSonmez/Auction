<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="469.0" prefWidth="645.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.auction.SessionSelection">
   <children>
      <Label layoutX="113.0" layoutY="54.0" text="Select a session: " />
      <TableView fx:id="sessionTable" layoutX="113.0" layoutY="99.0" prefHeight="272.0" prefWidth="419.0">
         <columns>
            <TableColumn fx:id="auctionDate" prefWidth="75.0" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnBack" layoutX="341.0" layoutY="391.0" mnemonicParsing="false" onAction="#btnBackAction" prefHeight="37.0" prefWidth="135.0" text="Back" />
      <Button fx:id="enterButton" layoutX="177.0" layoutY="391.0" mnemonicParsing="false" onAction="#enterButtonAction" prefHeight="37.0" prefWidth="135.0" text="Enter Session" />
   </children>
</AnchorPane>
